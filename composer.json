{
    "name": "dragu/google-vision",
    "description": "Google Vision Api for PHP (https://cloud.google.com/vision/) with word features fixed",
    "keywords": ["Google","Cloud", "Vision", "Api", "Image", "Face", "Recognition", "Landmark"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jordi Kroon",
            "email": "jordi@jordikroon.nl"
        }
    ],
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "~7.1",
        "laminas/laminas-hydrator": "~4.13.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3"
    },
    "autoload": {
        "psr-4": {
            "Vision\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vision\\Tests\\": "tests/"
        }
    }
}
